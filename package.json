{
  "name": "@queenanya/baileys",
  "version": "8.4.1",
  "description": "Custom Baileys WhatsApp API",
  "keywords": [
    "baileys",
    "baileys-mod",
    "wabot",
    "whatsapp",
    "js-whatsapp",
    "whatsapp-api",
    "whatsapp-web",
    "whatsapp-bot",
    "automation",
    "multi-device"
  ],
  "homepage": "https://github.com/nstar-y/bail",
  "repository": {
    "url": "git+ssh://git@github.com/QueenAnya/Bail.git"
  },
  "license": "MIT",
  "author": "Adhiraj Singh",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*",
    "WAProto/*.ts",
    "WAProto/*.js",
    "WASignalGroup/*.js",
    "engine-requirements.js"
  ],
  "scripts": {
    "build:all": "tsc && typedoc",
    "build:docs": "typedoc",
    "build:tsc": "tsc",
    "changelog:last": "conventional-changelog -p angular -r 2",
    "changelog:preview": "conventional-changelog -p angular -u",
    "changelog:update": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "example": "node --inspect -r ts-node/register Example/example.ts",
    "gen:protobuf": "sh WAProto/GenerateStatics.sh",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "yarn lint --fix",
    "prepack": "tsc",
    "prepare": "tsc",
    "preinstall": "node ./engine-requirements.js",
    "release": "release-it",
    "test": "jest"
  },
  "dependencies": {
    "@adiwajshing/keyed-db": "^0.2.4",
    "@cacheable/node-cache": "^1.4.0",
    "@hapi/boom": "^10.0.1",
    "async-lock": "^1.4.1",
    "async-mutex": "^0.5.0",
    "audio-decode": "^2.1.3",
    "axios": "^1.7.3",
    "cache-manager": "^5.7.6",
    "cheerio": "^1.0.0 || ^1.0.0_rc.12 || ^1.0.0-rc.10",
    "libphonenumber-js": "^1.10.20",
    "libsignal": "github:nstar-y/libsignal",
    "jimp": "^0.22.12",
    "lodash": "^4.17.21",
    "music-metadata": "^7.12.3",
    "pino": "^9.6",
    "protobufjs": "^7.2.4",
    "uuid": "^10.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@adiwajshing/eslint-config": "github:adiwajshing/eslint-config",
    "@queenanya/eslint-config": "github:adiwajshing/eslint-config",
    "@types/got": "^9.6.11",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@types/ws": "^8.5.11",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.7.0",
    "jest": "^29.7.0",
    "jimp": "^0.22.12",
    "json": "^11.0.0",
    "link-preview-js": "^3.0.5",
    "open": "^10.1.0",
    "protobufjs-cli": "^1.1.3",
    "qrcode-terminal": "^0.12.0",
    "release-it": "^17.6.0",
    "sharp": "^0.34.2",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.27.9",
    "typedoc-plugin-markdown": "4.4.2",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "audio-decode": "^2.1.3",
    "jimp": "^0.22.12",
    "link-preview-js": "^3.0.5",
    "qrcode-terminal": "^0.12.0",
    "sharp": "^0.34.2"
  },
  "peerDependenciesMeta": {
    "audio-decode": {
      "optional": true
    },
    "jimp": {
      "optional": true
    },
    "link-preview-js": {
      "optional": true
    },
    "sharp": {
      "optional": true
    }
  }
}
